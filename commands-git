git clone 'url'
git add 'les fichier que vous voulez ajouter'
git commit -m 'message de commit"
git push origin main
git pull origin main
#pour crer une branch
git branch 'nom de la branch'
#pour alle dans la branche
git checkout 'nom de la branch'
#pour sauvegarder la branch il faut aller dedans puis utiliser git push origin 'nom de la branch'
